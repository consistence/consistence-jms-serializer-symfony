{
	"name": "consistence/consistence-jms-serializer-symfony",
	"description": "Symfony Bundle integrating Consistence library with JMS Serializer",
	"license": "MIT",
	"authors": [
		{
			"name": "Va≈°ek Purchart",
			"email": "me@vasekpurchart.cz",
			"homepage": "http://vasekpurchart.cz"
		}
	],
	"require": {
		"php": "^7.2",
		"consistence/consistence-jms-serializer": "^2.0",
		"jms/serializer-bundle": "^3.0",
		"symfony/config": "^3.4|^4.0",
		"symfony/dependency-injection": "^3.4|^4.0",
		"symfony/http-kernel": "^3.4|^4.0",
		"symfony/yaml": "^3.4|^4.0"
	},
	"require-dev": {
		"consistence/coding-standard": "3.10.1",
		"matthiasnoback/symfony-dependency-injection-test": "4.3",
		"php-parallel-lint/php-console-highlighter": "1.0",
		"php-parallel-lint/php-parallel-lint": "1.3.2",
		"phing/phing": "2.17.2",
		"phpunit/phpunit": "8.5.25",
		"shipmonk/composer-dependency-analyser": "1.8.1"
	},
	"autoload": {
		"psr-4": {
			"Consistence\\JmsSerializer\\SymfonyBundle\\": "src"
		},
		"classmap": [ "src" ]
	},
	"autoload-dev": {
		"psr-4": {
			"Consistence\\JmsSerializer\\SymfonyBundle\\": "tests"
		},
		"classmap": [ "tests" ]
	},
	"config": {
		"bin-dir": "bin",
		"allow-plugins": {
			"dealerdirect/phpcodesniffer-composer-installer": false
		}
	}
}
